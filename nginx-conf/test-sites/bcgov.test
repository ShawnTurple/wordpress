server {
	listen 8089 ssl;
  server_name bcgov.test;
  error_log  /var/log/nginx/error.log;
  access_log /var/log/nginx/access.log;
  root /data/www-app/bcgov.test/web;

	ssl_certificate /data/ssl/certs/bcgov_test.crt;
	ssl_certificate_key /data/ssl/private/bcgov_test.key;

	include /data/apps/nginx/etc/global/wordpress-multinetwork.conf;
}

server {
	listen 8080;
	server_name bcgov.test;
	return 301 https://$server_name$request_uri;
}
